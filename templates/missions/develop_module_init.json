{
  "domain": "software",
  "skill": "developer_python",
  "contexts": [
    "design/general/product_goals",
    "workspace/implement_approach",
    "design/general/data_structures_and_interface_definitions"
  ],
  "tasks": [
    {% for module in python_module_list %}
    {
      "task_type": "Task",
      "name": "develop/python_pytest",
      "sub_name": "test_{{ module[0] }}.py",
      "title": "Unit Test: test_{{ module[0] }}.py",
      "target": "code/tests/test_{{ module[0] }}.py",
      "required_contexts": [
        "design/general/product_goals",
        "workspace/implement_approach",
        "design/general/data_structures_and_interface_definitions",
        "code/{{ module[0] }}.py"
      ]
    },
    {
      "task_type": "Task",
      "name": "develop/python_general",
      "sub_name": "{{ module[0] }}.py",
      "title": "Code: {{ module[0] }}.py",
      "target": "code/{{ module[0] }}.py",
      "required_contexts": [
        "design/general/product_goals",
        "workspace/implement_approach",
        "design/general/data_structures_and_interface_definitions"
      ]
    }{{ ", " if not loop.last else "" }}
    {% endfor %}
  ]
}